{
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime"
    ],
    "presets": [
        "@babel/preset-env",
        [
            
            "@babel/preset-react",
            {
                "flow":false,
                "typescript":true
            }
            
        ],

        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true
            }
        ]
    ]
}